{
  "manifest_version": 2,

  "name": "T to B",
  "description": "This extension replaces images of Donald Trump with Bo Obama",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Click here!"
  },

  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ],

  "content_scripts": [{
    "matches": ["http://*/"],
    "js": ["sub.js"],
    "run_at": "document_idle"
  }],

  "web_accessible_resources": ["/images/*"]
}